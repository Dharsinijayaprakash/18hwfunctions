//argument passed,no return
//call by value
#include <stdio.h>
void main() 
{
    int u;
    printf("enter a number : ");
    scanf("%d",&u);
    printf("given value is %d\n",u);
    dec(u);//actual parameter
}
void dec(int u)//formal parameter
{
    u=u-1;
    printf("decremented value is %d",u);
}
